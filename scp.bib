  keywords = {Ionic liquid electrolyte,Aluminum-ion battery,Dual-graphite,High discharge voltage},
  pages = {119-127},
  file = {C\:\\Users\\divyash\\Zotero\\storage\\CEVN946G\\Wang et al. - 2018 - A novel dual-graphite aluminum-ion battery.pdf;C\:\\Users\\divyash\\Zotero\\storage\\QDQ2B6EY\\S2405829717305755.html},
  ids = {wang\_novel\_2018}
}

@article{li_novel_nodate,
