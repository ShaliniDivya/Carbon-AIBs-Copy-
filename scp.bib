@article{gnanamuthu_electrochemical_2011,
  title = {Electrochemical Properties of {{Super P}} Carbon Black as an Anode Active Material for Lithium-Ion Batteries},
  volume = {130},
  issn = {0254-0584},
  abstract = {A new approach to investigate upon the electrochemical properties of Super P carbon black anode material is attempted and compared with conventional mesophase pitch-based carbon fibers (MPCFs) anode material for lithium-ion batteries. The prepared Super P carbon black electrodes are characterized using transmission electron microscope (TEM). The assembled 2032-type coin cells are electrochemically characterized by ac impedance spectroscopic and cyclic voltammetric methods. The electrochemical performance of charge and discharge was analyzed using a battery cycler at 0.1C rate and cut-off potentials of 1.20 and 0.01V vs. Li/Li+. The electrochemical test illustrates that the discharge capacity corresponding to Li intercalation into the Super P carbon black electrode is higher and coulombic efficiency is maintained approximately 84\% at the end of the 20th cycling at room temperature.},
  number = {3},
  journal = {Materials Chemistry and Physics},
  doi = {10.1016/j.matchemphys.2011.08.060},
